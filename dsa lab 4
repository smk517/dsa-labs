#include<iostream>
using namespace std;

struct Node {
    int data;
    Node* next;
};

void print_data(Node* head) {
    if (head == NULL) {
        cout << "Linked list is empty";
        return;
    }
    Node* ptr = head;
    while (ptr != NULL) {
        cout << ptr->data << " ";
        ptr = ptr->next;
    }
    cout << endl;
}

Node* at_end(Node* head, int newvalue) {
    Node* ptr = new Node();
    ptr->data = newvalue;
    ptr->next = NULL;
    
    if (head == NULL) {
        return ptr;
    }
    
    Node* temp = head;
    while (temp->next != NULL) {
        temp = temp->next;
    }
    temp->next = ptr;
    
    return head;
}

int main() {
    int newvalue;
    Node node1, node2, node3, node4;
    node1.data = 10;
    node1.next = &node2;
    node2.data = 20;
    node2.next = &node3;
    node3.data = 30;
    node3.next = &node4;
    node4.data = 40;
    node4.next = NULL;
    
    Node* head = &node1;
    
    cout << "Enter the new value to insert at the end:" << endl;
    cin >> newvalue;
    
    head = at_end(head, newvalue);
    
    cout << "Linked list values after insertion are:" << endl;
    print_data(head);
    
    return 0;
}

