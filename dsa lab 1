
#include<iostream>
using namespace std;
int main(){
	int size=8;
	int arr[8]={1,2,3,4,5,6,7,8};
	cout<<"the array is:"<<endl;
	for(int i=0;i<size;i++){
		cout<<arr[i]<<endl;
	}
	cout<<"the array after removing element from start"<<endl;
	for(int i=1;i<size;i++){
		cout<<arr[i]<<endl;
	}
	cout<<"the array after removing element from end"<<endl;
	for(int i=0;i<size-1;i++){	
	cout<<arr[i]<<endl;
	
	}
	return 0;
}
