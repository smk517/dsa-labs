#include<iostream>
using namespace std;
int main(){
	int arr[5]={1,2,3,4,5};
	int *p=arr;
	for(int i=0;i<5;i++){
		cout<<"value"<<i<<":"<<*p<<endl;
		
		p++;
		
	}
	return 0;
}


	#include<iostream>
using namespace std;
void swap(int *a,int *b){
	int c;
	c=*a;
	*a=*b;
	*b=c;
	cout<<"after swaping values a is:"<<endl;
	cout<<*a<<endl;
	cout<<"after swaping values b is:"<<endl;
	cout<<*b<<endl;
	
}
int main(){
	int a=25;
	int b=35;
	cout<<"before the swap number are"<<endl;
	cout<<"a is:"<<endl;
	cout<<a<<endl;
	cout<<"b is:"<<endl;
	cout<<b<<endl;
	swap(&a,&b);
	return 0;
}


